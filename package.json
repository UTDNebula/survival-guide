{
  "name": "nebula-guide",
  "private": true,
  "description": "UTD Survival Guide",
  "version": "0.1.0",
  "author": "ACM Development",
  "dependencies": {
    "@types/react-helmet": "^6.1.2",
    "gatsby": "^3.10.1",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-feed": "^3.10.0",
    "gatsby-plugin-gatsby-cloud": "^2.10.0",
    "gatsby-plugin-google-analytics": "^3.10.0",
    "gatsby-plugin-image": "^1.10.1",
    "gatsby-plugin-manifest": "^3.10.0",
    "gatsby-plugin-netlify-cms": "^5.10.0",
    "gatsby-plugin-offline": "^4.10.0",
    "gatsby-plugin-react-helmet": "^4.10.0",
    "gatsby-plugin-sharp": "^3.10.2",
    "gatsby-remark-copy-linked-files": "^4.7.0",
    "gatsby-remark-images": "^5.7.0",
    "gatsby-remark-prismjs": "^5.7.0",
    "gatsby-remark-responsive-iframe": "^4.7.0",
    "gatsby-remark-smartypants": "^4.7.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-remark": "^4.7.0",
    "gatsby-transformer-sharp": "^3.10.0",
    "netlify-cms-app": "^2.15.30",
    "prismjs": "^1.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.75"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "autoprefixer": "^10.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gatsby-plugin-postcss": "^3.7.0",
    "gatsby-plugin-ts": "^2.7.1",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.2.2"
  },
  "homepage": "https://github.com/acmutd/nebula-guide#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/acmutd/nebula-guide"
  },
  "bugs": {
    "url": "https://github.com/acmutd/nebula-guide/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,js}": [
      "eslint --cache --fix .",
      "prettier --write src/**/*.{tsx,ts,js}"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "disableSubjectLowerCase": true,
      "maxHeaderWidth": 72,
      "maxLineWidth": 80,
      "defaultType": "chore"
    }
  }
}
